{
	"name": "text-convex-trello",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"dev": "bunx convex dev --once && concurrently -r bun:dev:web bun:dev:db",
		"dev:web": "vite dev",
		"dev:db": "convex dev --run board:seed",
		"build": "vite build && tsc --noEmit",
		"start": "bun start",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"check": "biome check .",
		"check:fix": "biome check --write ."
	},
	"dependencies": {
		"@convex-dev/react-query": "0.0.0-alpha.8",
		"@tanstack/react-query": "^5.66.0",
		"@tanstack/react-query-devtools": "^5.66.0",
		"@tanstack/react-router": "^1.132.17",
		"@tanstack/react-router-ssr-query": "^1.132.17",
		"@tanstack/react-router-devtools": "^1.132.17",
		"@tanstack/react-start": "^1.132.18",
		"concurrently": "^8.2.2",
		"convex": "^1.19.0",
		"ky": "^1.7.4",
		"msw": "^2.7.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-hot-toast": "^2.5.1",
		"redaxios": "^0.5.1",
		"tailwind-merge": "^2.6.0",
		"tiny-invariant": "^1.3.3",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.2",
		"@types/node": "22.10.2",
		"@types/react": "^19.0.8",
		"@types/react-dom": "^19.0.3",
		"@vitejs/plugin-react": "^4.3.4",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.5.1",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.7.2",
		"vite": "^7.1.7",
		"vite-tsconfig-paths": "^5.1.4"
	}
}
